generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  email String  @unique
  name  String?
  // address Address?
  // posts   Post[]
}

type Address {
  street String
  city   String
  state  String
  zip    String
}
