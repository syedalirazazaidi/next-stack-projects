generator client {
  provider = "prisma-client-js"
}

// FJgEq6BNpUSFqCPB
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Post {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  title       String?
  description String?
  createdDate DateTime @default(now())
  updatedDate DateTime @updatedAt
  // address Address?
  // posts   Post[]
}

